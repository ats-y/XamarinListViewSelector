        Orientation="Horizontal"
        Padding="10,10,0,10"
        BackgroundColor="#96ada9">
        <Label
            Text="小分類：" />
        <Label
            Text="{Binding SubCategoryName.Value}" />
    </StackLayout>
</ViewCell>
